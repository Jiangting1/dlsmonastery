<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.myspring.cloud.modules.kingdee.mapper.ArReceivableMapper">
    <select id="findFBillNoByfSourceBillNo" resultType="String">
      select
       top 1 t1.FBILLNO
       from
       T_AR_RECEIVABLE t1,
       T_AR_RECEIVABLEENTRY t2
       where
       t1.fid=t2.FID
       and t2.FSOURCEBILLNO = #{fSourceBillNo}
    </select>
</mapper>
