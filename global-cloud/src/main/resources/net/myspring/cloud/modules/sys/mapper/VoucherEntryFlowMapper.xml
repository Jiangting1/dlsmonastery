<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.myspring.cloud.modules.sys.mapper.VoucherEntryFlowMapper">
    <select id="findByVoucherEntryId" resultType="VoucherEntryFlow">
        SELECT
        t1.*
        from
        sys_gl_voucher_entry_flow t1
        WHERE
        t1.gl_voucher_entry_id = #{voucherEntryId}
    </select>
</mapper>
