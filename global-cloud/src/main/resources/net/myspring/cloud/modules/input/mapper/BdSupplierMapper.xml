<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.myspring.cloud.modules.input.mapper.BdSupplierMapper">
    <select id="findAll" resultType="BdSupplier">
       select
       t1.FSUPPLIERID,
       t1.FNUMBER,
       t2.FNAME
       from
       T_BD_SUPPLIER t1,T_BD_SUPPLIER_L t2
       where
       t1.FSUPPLIERID = t2.FSUPPLIERID
    </select>
</mapper>
