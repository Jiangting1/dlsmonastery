<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.myspring.future.modules.crm.mapper.DealerMapper">

    <select id="findPage" resultType="Dealer">
        select t1.*
        from crm_dealer t1
        where
        t1.enabled=1
        <if test="p.name !=null">
            AND t1.name like concat('%',#{p.name},'%')
        </if>
    </select>

</mapper>
