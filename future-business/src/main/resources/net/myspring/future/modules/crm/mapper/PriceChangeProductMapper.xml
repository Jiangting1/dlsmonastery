<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.myspring.future.modules.crm.mapper.PriceChangeProductMapper">

    <select id="findByPriceChangeId" resultType="PriceChangeProduct">
        select t1.* from crm_price_change_product t1 where t1.price_change_id = #{priceChangeId}
    </select>

</mapper>
