<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.myspring.basic.modules.hr.mapper.EmployeePointMapper">

    <select id="findPage" resultType="EmployeePoint">
        SELECT
            t1.*
        FROM
        hr_employee_point t1
        WHERE
        t1.enabled=1
    </select>
    
</mapper>
