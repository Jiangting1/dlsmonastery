<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.myspring.future.modules.crm.mapper.GoodsOrderImeMapper">

    <select id="findByGoodsOrderId" resultType="GoodsOrderIme">
        select t1.*
        from crm_goods_order_ime t1
        where
          t1.goods_order_Id = #{goodsOrderId}
          and t1.enabled=1
    </select>


</mapper>
